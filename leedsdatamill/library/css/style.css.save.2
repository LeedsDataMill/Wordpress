body {
	background: #fff;
	padding-top: 0;
}

main {
	background: #f2f2f2;
	padding-bottom: 60px;
}

footer {
	background: #FFF;
	padding-top: 20px;
}

body.single h2 {
	padding: 40px 0;

}

#main article {
	background: #FFF;
	border-bottom: 0;
	margin-bottom: 0;
	padding-bottom: 0;
}

#main article header .page-header {
	padding: 0 20px 0;
	margin-bottom: 0;
	border: 0;
}

#main article header p.meta {
	padding: 0 20px;
}

#main article img.attachment-wpbs-featured {
	width: 100%;
}

#main article section.post_content {
	padding: 20px;
}

#main article footer {
	background: #f2f2f2;
	padding: 20px 0;
}

.tags {
	padding: 20px 0;
}

.tags .tags-title {
	text-transform: uppercase;
}

.tags a {
	background: #81519a;
	padding: .8em 2em;
	border-radius: 0;
}

@media (min-width: 768px) {
  .navbar-nav {
    float: right;
  }
}

/*
font-family: 'Roboto Slab', serif;
font-family: 'Open Sans', sans-serif;
 */

body {
	font-family: 'Open Sans', sans-serif;
}

.site-footer {
	background: #FFF;
	min-height: 100%;
}

.container {
	position: relative;
}

.main.homepage{
	padding: 0;
}

.white-bg {
	background: #FFF;
}

.no-bullets {
	list-style: none;
	margin-left: 0;
}

a {
	color: #333;
}

p.photo-credit, .alt p.photo-credit, p.photo-credit a, .alt p.photo-credit a {
	color: #46255f;
}

p.photo-credit {
	position: absolute;
	right: 0;
	bottom: 0;
	background: #FFF;
	padding: 10px;
	margin: 0;
	font-weight: bold;
}

.account-masthead {
	background: #333;
	padding: 10px 0;
}

.masthead {
	background: #381B4B;
}

.pages-nav {
	background: #FFF;
	padding: 20px 0;
}

.pages-nav .container {
	position: relative;
}

.pages-nav .content {
	position: absolute;
	right: 0;
	top: -10px;
}

.pages-nav .header-image img {
	height: 27px;
}

.pages-nav li {

}

.pages-nav li a {
	font-size: .96em;
	font-weight: bold;
	display: block;
	padding: 15px 6px 0;
	color: #542f68;
	border-radius: 0;
	text-transform: uppercase;
}

.pages-nav .btn.btn-text {
	color: #333;
	border: 1px solid #333;
	padding: 13px 15px;
	font-size: .96em;
	margin-left: 6px;
}

.pages-nav .navigation .nav-pills li a:hover, .pages-nav .navigation .nav-pills li.active a {
	background: none;
}

h1 {
	font-family: 'Roboto Slab', serif;
	text-transform: uppercase;

	line-height: 1.2em;
}

h2 {
	clear: both;
}

h2 {
	text-transform: uppercase;
	font-family: 'Roboto Slab', serif;
	font-size: 2.25em;
	font-weight: normal;
	color: #333;
	margin-top: 0;
}

h2 span.highlight {
	display: table;
	background: #381b4b;
	color: #FFF;
	padding: 4px;
	line-height: 1;
}

.site-footer h4 {
	text-transform: uppercase;
	font-family: 'Roboto Slab', serif;
	font-size: .96em;
	line-height: 40px;
	font-weight: bold;
	color: #381b4b;
	margin: 0;
}

.white-bg h1, .white-bg h2, .white-bg h3, .white-bg h4, .white-bg h5, .white-bg h6, .white-bg li, .white-bg p {
	color: #333;
}

.alt h3 {
	color: #FFF;
}

.alt h3 span.highlight {
	background: #FFF;
	color: #000;
}

.alt h4, .alt p, .alt li, .alt label {
	color: #FFF;
}

h4 {
	font-weight: bold;
	margin-bottom: 1em;
}

.uppercase {
	text-transform: uppercase;
}

p {
	margin-bottom: .86em;
}

.radio input[type="radio"], .checkbox input[type="checkbox"] {
	top: 4px;
}

fieldset[class*="span"] {
	margin: 0;
}

.tag {
	font-weight: bold;
	border-radius: 0;
	color: #FFF;
	border: 0 none;
	background: #81519a;
	padding: 8px;
	text-transform: lowercase;
	box-shadow: none;
	-webkit-box-shadow: none;
}

a.tag:hover {
	border: 0;
	background: #333;
	box-shadow: none;
	-webkit-box-shadow: none;
}

.btn.btn-text {
	color: #FFF;
	background: transparent;
	border: 2px solid #FFF;
	padding: 13px 50px;
	text-transform: uppercase;
	font-size: 15px;
	font-family: 'Roboto Slab';
	font-weight: bold;

	-moz-text-shadow: none;
	-webkit-text-shadow: none;
	text-shadow: none;

	-moz-border-radius: 0;
	-webkit-border-radius: 0;
	border-radius: 0;
}

.btn {
	background: #4d2d63;
	color: #FFF;
}

.navbar-default {
	margin-bottom: 0;
}

.btn.btn-primary,
.btn.btn-mini {
	background:#381b4b;
	border: 2px solid #FFF;
}

body.page-template-template-contact-php main {
	background: url('../img/contact-bg.jpg') no-repeat top center;
}

body.page-template-template-contact-php #main article,
body.page-template-template-contact-php #main article footer {
        background: none;
}

body.page-template-template-contact-php h1 {
	color: #FFF;
}

body.page-template-template-contact-php form p {
	color: #FFF;
	text-transform: uppercase;
}

.archive h2 {
	padding: 0 20px 20px 0;
}

.archive #main article section.post_content {
	padding-top: 0;
}

.archive #main article img.attachment-wpbs-featured {
	margin-bottom: 20px;
}

.single article nav {
	background: #f2f2f2;
	width: 100%;
	float: left;
	padding-bottom: 40px;
}

.single article nav a {

width: 40%;
display: block;
background: #3c214e;
padding: 18px;
color: #FFF;
text-align: center;
text-tra	nsform: uppercase;
	font-family: 'Roboto Slab';
